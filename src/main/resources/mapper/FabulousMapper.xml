<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.art1001.supply.mapper.fabulous.FabulousMapper">
	
	<resultMap id="BaseResultMap" type="com.art1001.supply.entity.fabulous.Fabulous">
		<result column="fabulous_id" property="fabulousId" jdbcType="BIGINT" />
		<result column="member_id" property="memberId" jdbcType="VARCHAR" />
		<result column="public_id" property="publicId" jdbcType="VARCHAR" />
	</resultMap>
    
	<sql id="Base_Column_List">
						fabulous_id,
				member_id,
				public_id
	</sql>

	<sql id="whereStatement">
		<where>
			1 = 1
			<if test="condition !=null">
			</if>
		</where>
	</sql>

	<!-- 删除多个信息的所有赞 -->
	<delete id="deleteManyFabulousByInfoId">
		delete from prm_fabulous where public_id in
		(
			<foreach collection="list" separator="," item="publicId">
				#{publicId}
			</foreach>
		)
	</delete>

	
</mapper>